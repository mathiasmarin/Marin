@{
    ViewData["Title"] = "Om dig";
}
@section Scripts
{
    <script src="~/js/About.js"></script>
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<div id="testPage" class="container" v-cloak>
    <h2>Användare</h2>
    <table class="table table-striped table-bordered">
        <thead>
        <tr>
            <th>Förnamn</th>
            <th>Efternamn</th>
            <th>E-post</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>{{user.firstName}}</td>
            <td>{{user.lastName}}</td>
            <td>{{user.email}}</td>
        </tr>
        </tbody>
    </table>
</div>
